@{
    ViewBag.Title = "Envios Salientes";
}
@section enviosSalientesDataTable
{
    <script src="@Url.Content("~/Scripts/jQuery.dataTables.min.js")"></script>
    <link href="@Url.Content("~/Scripts/jquery.dataTables.min.css")" rel="stylesheet" type="text/css" />

    <script src="@Url.Content("~/Scripts/bootstrap-dialog.min.js")"></script>
    <link href="@Url.Content("~/Scripts/bootstrap-dialog.min.css")" rel="stylesheet" type="text/css" />

    <script src="@Url.Content("~/Content/leaflet.js")"></script>
    <link href="@Url.Content("~/Content/leaflet.css")" rel="stylesheet" type="text/css" />

    <link href="@Url.Content("~/Content/table.css")" rel="stylesheet" type="text/css" />

    <script src="@Url.Content("~/Scripts/enviosSalientes-table.js")"></script>

    <script src="@Url.Content("~/Scripts/jszip.min.js")"></script>
    <script src="@Url.Content("~/Scripts/jszip.min.js")"></script>
    <script src="@Url.Content("~/Scripts/dataTables.buttons.min.js")"></script>
    <script src="@Url.Content("~/Scripts/pdfmake.min.js")"></script>
    <script src="@Url.Content("~/Scripts/vfs_fonts.js")"></script>
    <script src="@Url.Content("~/Scripts/buttons.html5.min.js")"></script>
    <script src="@Url.Content("~/Scripts/buttons.print.min.js")"></script>
    <link href="@Url.Content("~/Scripts/buttons.jqueryui.min.css")" rel="stylesheet" type="text/css" />
}
<div class="container-fluid">
    <div class="modal fade">
        <div class="modal-dialog">
            <div class="modal-content">
                <div class="modal-header">
                    <button type="button" class="close" data-dismiss="modal" aria-hidden="true">×</button>
                    <h4 class="modal-title">Modal title</h4>
                </div>
                <div class="modal-body">
                    <p>One fine body…</p>
                </div>
                <div class="modal-footer">
                    <button type="button" class="btn btn-default" data-dismiss="modal">Cancel</button>
                    <button type="button" class="btn btn-primary">Confirmar</button>
                </div>
            </div><!-- /.modal-content -->
        </div><!-- /.modal-dialog -->
    </div><!-- /.modal -->
    <!-- Page Heading -->
    <div class="row">
        <div class="col-lg-12">
            <h1 class="page-header">
                Envios Salientes
            </h1>
        </div>
    </div>
    <!-- /.row -->
    <div id="modal-container" class="modal fade" tabindex="-1" role="dialog"></div>
    <div class="row">
        <div>
            <label>
                @Html.DropDownList("ddlTransportistas", (IEnumerable<SelectListItem>)TempData["TransportistasDDLText"], "Transportista", new { @class = "form-control" })
            </label>
        </div>
        <br />
        <div id="container">
            <table id="enviosSalientesDataTable" class="display">
                <thead>
                    <tr>
                        <th>IdEnvio</th>
                        <th>DescTransportista</th>
                        <th>DescCLiente</th>
                        <th>FechaCarga</th>
                        <th>Observaciones</th>
                        <th>Contactos</th>
                    </tr>
                </thead>
                <tbody></tbody>
            </table>
        </div>
    </div>
</div>
<!-- /.container-fluid -->